   "source": [
    "def time_it(func):\n",
    "    def wrapper(*args):\n",
    "        start_t = time.time()\n",
    "        func(*args)\n",
    "        end_t = time.time()\n",
    "        time_taken = end_t - start_t\n",
    "        print(f\"Time taken : {time_taken}\")\n",
    "    return wrapper\n"
   ]
  }
